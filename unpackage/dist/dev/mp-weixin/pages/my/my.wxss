
@font-face {font-family: "iconfont";
  src : url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2');}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-bofangjilu:before {
  content: "\e8ad";
}
.icon-wodeguanzhu:before {
  content: "\e8bc";
}
.icon-xiazai:before {
  content: "\e8bd";
}
.icon-yixiangkan:before {
  content: "\e8bf";
}
.clear:after{
    content: "";
    display: block;
    clear: both;
}
.left{
    float: left;
}
.right{
    float: right;
}
.aa{
	margin: 5px;
	border-radius: 5px;
	border: 1px solid #cfd1c8;
	padding: 10px 20px;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.aa image{
	width: 20%;
	border-radius: 50%;
}
.bb{
	width: 100%;
	margin: 0 auto;
	background-color: #f55500;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-wrap: nowrap;
	        flex-wrap: nowrap;
	white-space: nowrap;
}
.bb .bb_1{
	width: 30%;
	margin: 20rpx 10rpx;
	text-align: center;
	color: #ffffff;
	display: inline-block;
}
.cc{
	padding: 5px 10px;
	border-bottom: 1px solid #f7f7f7;
}

