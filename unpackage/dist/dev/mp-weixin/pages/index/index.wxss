
.clear:after{
    content: "";
    display: block;
    clear: both;
}
.left{
    float: left;
}
.right{
    float: right;
}
.aa{
	display:-webkit-box;
	display:-webkit-flex;
	display:flex;
	height: 100rpx;
	background-color: #f55500;
}
.aa_1{
	width: 90%;
	text-align: center;
	border: 1px solid #f0f0f0;
	border-radius: 10rpx;
	margin-top: 20rpx;
	margin-left: 20rpx;
	background-color: #ffffff;
}
.bb{
	width: 100%;
	margin: 0 auto;
	background-color: #f55500;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-wrap: nowrap;
	        flex-wrap: nowrap;
	white-space: nowrap;
}
.bb .bb_1{
	width: 30%;
	margin: 20rpx 10rpx;
	text-align: center;
	color: #ffffff;
	display: inline-block;
}
.cc{
	padding: 10px 10px;
}
.cc_1{
	width: 30%;
	height: 220rpx;
	line-height: 320rpx;
	color: #ffffff;
	text-align: center;
	background: url(data:image/jpeg;base64,) no-repeat;
	background-size:cover;
	box-shadow:0px 0px 5px 5px #dfdfdf;
}
.cc_1 image{
	width: 100rpx;
	height: 100rpx;
}
.cc_2{
	width: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	background-color: #f3f1f1;
	margin-bottom: 20rpx;
}
.cc_2 image{
	width: 100rpx;
	height: 100rpx;
}
.dd_1{
	margin-left: 10px;
	display: inline-block;
}
.dd_1 image{
	width: 15%;
	height: 48px;
}

